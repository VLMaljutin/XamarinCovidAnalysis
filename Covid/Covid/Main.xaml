<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Covid.Main"
            xmlns:windows="clr-namespace:Xamarin.Forms.PlatformConfiguration.WindowsSpecific;assembly=Xamarin.Forms.Core" xmlns:local="clr-namespace:Covid.ForMain"
            windows:TabbedPage.HeaderIconsEnabled="true">
    
    <TabbedPage.Children>
        <NavigationPage Title="Казань" IconImageSource="kazan.png">
            <x:Arguments>
                <local:Kazan/>
            </x:Arguments>
        </NavigationPage>
        <NavigationPage Title="Москва">
            <x:Arguments>
                <local:Moscow/>
            </x:Arguments>
        </NavigationPage>
    </TabbedPage.Children>
    
</TabbedPage>